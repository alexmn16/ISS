<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.ListView?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="530.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/17" xmlns:fx="http://javafx.com/fxml/1" fx:controller="MainControllerProgrammer">
   <children>
      <Label fx:id="welcomeLabel" layoutX="110.0" layoutY="14.0" prefHeight="65.0" prefWidth="379.0" text="Welcome back, " textFill="#099125">
         <font>
            <Font name="System Italic" size="12.0" />
         </font>
      </Label>
      <Button fx:id="viewBugsButton" layoutX="191.0" layoutY="91.0" mnemonicParsing="false" onMouseClicked="#viewBugsButtonClicked" text="View Bugs" />
      <Button fx:id="deleteBugButton" layoutX="476.0" layoutY="299.0" mnemonicParsing="false" onMouseClicked="#deleteButtonClicked" text="Delete Bug" />
      <Button fx:id="updateBugButton" layoutX="473.0" layoutY="203.0" mnemonicParsing="false" onMouseClicked="#updateButtonClicked" text="Update Bug" />
      <ListView fx:id="bugsList" layoutX="45.0" layoutY="124.0" prefHeight="303.0" prefWidth="363.0" />
      <Button fx:id="changePasswordButton" layoutX="171.0" layoutY="477.0" mnemonicParsing="false" onMouseClicked="#changePasswordButtonClicked" text="Change Password" />
      <TextField fx:id="passwordTextBox" layoutX="152.0" layoutY="436.0" promptText="New password..." />
   </children>
</AnchorPane>
