<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.ListView?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="497.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/17" xmlns:fx="http://javafx.com/fxml/1" fx:controller="MainControllerTester">
   <children>
      <Label fx:id="welcomeLabel" layoutX="110.0" layoutY="14.0" prefHeight="65.0" prefWidth="379.0" text="Welcome back, " textFill="#099125">
         <font>
            <Font name="System Italic" size="12.0" />
         </font>
      </Label>
      <Button fx:id="viewBugsButton" layoutX="153.0" layoutY="79.0" mnemonicParsing="false" onMouseClicked="#viewBugsButtonClicked" text="View Bugs" />
      <Button fx:id="addBugButton" layoutX="435.0" layoutY="302.0" mnemonicParsing="false" onMouseClicked="#addButtonClicked" text="Add Bug" />
      <ListView fx:id="bugsList" layoutX="33.0" layoutY="124.0" prefHeight="265.0" prefWidth="311.0" />
      <TextField fx:id="descriptionTextBox" layoutX="391.0" layoutY="257.0" promptText="Description..." />
      <TextField fx:id="nameTextBox" layoutX="391.0" layoutY="209.0" promptText="Name..." />
      <TextField fx:id="passwordTextBox" layoutX="114.0" layoutY="412.0" promptText="New password..." />
      <Button fx:id="passwordButton" layoutX="133.0" layoutY="451.0" mnemonicParsing="false" onMouseClicked="#changePasswordButtonClicked" text="Change Password" />
   </children>
</AnchorPane>
